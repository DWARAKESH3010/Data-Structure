   ],
   "source": [
    "li = [-1,0,1,2,8,9]\n",
    "l = 0\n",
    "r = len(li)-1\n",
    "target = 10\n",
    "while l <= r:\n",
    "    m = (l + r)//2\n",
    "    if li[m] == target:\n",
    "        print(m) \n",
    "        break\n",
    "    elif li[m] < target:\n",
    "        l = m + 1\n",
    "    elif li[m] > target:\n",
    "        r = m - 1\n",
    "else:\n",
    "    print(-1)"
   ]
